{
    "name": "lcobucci/di-builder",
    "description": "Dependency Injection Builder for PHP 5.3+ applications",
    "type": "library",
    "authors": [
        {
            "name": "Luís Otávio Cobucci Oblonczyk",
            "email": "lcobucci@gmail.com"
        }
    ],
    "keywords": [
        "dependency-injection"
    ],
    "homepage": "https://github.com/lcobucci/di-builder",
    "license": [
        "BSD-3-Clause"
    ],
    "require": {
        "php": ">=5.5",
        "symfony/config": "~2.7",
        "symfony/dependency-injection": "~2.7",
        "symfony/expression-language": "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "*",
        "symfony/proxy-manager-bridge": "~2.7",
        "mikey179/vfsStream": "~1.5"
    },
    "suggest": {
        "symfony/proxy-manager-bridge": "Allows the creation of lazy services"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
