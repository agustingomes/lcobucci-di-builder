{
    "name": "lcobucci/di-builder",
    "description": "Dependency Injection Builder for PHP 7.2+ applications",
    "type": "library",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "keywords": [
        "dependency-injection"
    ],
    "homepage": "https://github.com/lcobucci/di-builder",
    "license": [
        "BSD-3-Clause"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/expression-language": "^4.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^4.0",
        "infection/infection": "^0.7",
        "mikey179/vfsStream": "^1.6",
        "phpstan/phpdoc-parser": "^0.3@dev",
        "phpstan/phpstan": "^0.10@dev",
        "phpstan/phpstan-phpunit": "^0.10@dev",
        "phpstan/phpstan-strict-rules": "^0.10@dev",
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/proxy-manager-bridge": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "suggest": {
        "symfony/proxy-manager-bridge": "Allows the creation of lazy services"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.5-dev"
        }
    }
}
