{
    "name": "lcobucci/di-builder",
    "description": "Dependency Injection Builder for PHP 7.1+ applications",
    "type": "library",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "keywords": [
        "dependency-injection"
    ],
    "homepage": "https://github.com/lcobucci/di-builder",
    "license": [
        "BSD-3-Clause"
    ],
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.3|^4.0",
        "symfony/dependency-injection": "^3.3|^4.0",
        "symfony/expression-language": "^3.3|^4.0"
    },
    "require-dev": {
        "humbug/humbug": "dev-master@dev",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1",
        "symfony/proxy-manager-bridge": "^3.3|^4.0",
        "symfony/yaml": "^3.3|^4.0"
    },
    "suggest": {
        "symfony/proxy-manager-bridge": "Allows the creation of lazy services"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    }
}
