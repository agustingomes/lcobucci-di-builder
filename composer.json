{
    "name": "lcobucci/di-builder",
    "description": "Dependency Injection Builder for PHP 7.2+ applications",
    "type": "library",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "keywords": [
        "dependency-injection"
    ],
    "homepage": "https://github.com/lcobucci/di-builder",
    "license": [
        "BSD-3-Clause"
    ],
    "require": {
        "php": "^7.2",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/expression-language": "^4.1"
    },
    "require-dev": {
        "infection/infection": "^0.11",
        "lcobucci/coding-standard": "^2.0",
        "mikey179/vfsStream": "^1.6",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-deprecation-rules": "^0.10.2",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpstan/phpstan-strict-rules": "^0.10",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/proxy-manager-bridge": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "suggest": {
        "symfony/proxy-manager-bridge": "Allows the creation of lazy services"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.6-dev"
        }
    }
}
