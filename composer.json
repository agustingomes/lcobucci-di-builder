{
    "name": "lcobucci/di-builder",
    "description": "Dependency Injection Builder for PHP 7.1+ applications",
    "type": "library",
    "authors": [
        {
            "name": "Luís Otávio Cobucci Oblonczyk",
            "email": "lcobucci@gmail.com"
        }
    ],
    "keywords": [
        "dependency-injection"
    ],
    "homepage": "https://github.com/lcobucci/di-builder",
    "license": [
        "BSD-3-Clause"
    ],
    "require": {
        "php": "~7.1",
        "symfony/config": "~3.2",
        "symfony/dependency-injection": "~3.2",
        "symfony/expression-language": "~3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "~2.7",
        "symfony/proxy-manager-bridge": "~3.2",
        "mikey179/vfsStream": "~1.6",
        "humbug/humbug": "dev-master@dev"
    },
    "suggest": {
        "symfony/proxy-manager-bridge": "Allows the creation of lazy services"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    }
}
